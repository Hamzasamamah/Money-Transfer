@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}





<div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            <div class="col-md-12 grid-margin">
              <div class="row">
                <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                        @{
                            string adminName = ViewContext.HttpContext.Session.GetString("AdminName");
                        }
                        <h3 class="font-weight-bold">Welcome @adminName</h3>
                </div>
              </div>
            </div>
          </div>

        <!--Number Of Start-->
          <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
              <div class="card tale-bg">
                <div class="card-people mt-auto">
                  <img src="~/AdminAssets/images/dashboard/people.svg" alt="people">
                  <div class="weather-info">
                    <div class="d-flex">
                      <div>
                        <h2 class="mb-0 font-weight-normal"><i class="icon-sun mr-2"></i>31<sup>C</sup></h2>
                      </div>
                      <div class="ml-2">
                        <h4 class="location font-weight-normal">Amman</h4>
                        <h6 class="font-weight-normal">Jordan</h6>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-6 grid-margin transparent">
              <div class="row">
                <div class="col-md-6 mb-4 stretch-card transparent">
                  <div class="card card-tale">
                    <div class="card-body">
                      <p class="mb-4">number of Users</p>
                                <p class="fs-30 mb-2">@ViewBag.numberOfUsers</p>
                    </div>
                  </div>
                </div>
                <div class="col-md-6 mb-4 stretch-card transparent">
                  <div class="card card-dark-blue">
                    <div class="card-body">
                      <p class="mb-4">number of Banks</p>
                                <p class="fs-30 mb-2">@ViewBag.numberOfBanks</p>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6 mb-4 mb-lg-0 stretch-card transparent">
                  <div class="card card-light-blue">
                    <div class="card-body">
                      <p class="mb-4">Number of Wallets</p>
                      <p class="fs-30 mb-2">@ViewBag.numberOfWallets</p>
                    </div>
                  </div>
                </div>
                <div class="col-md-6 stretch-card transparent">
                  <div class="card card-light-danger">
                    <div class="card-body">
                      <p class="mb-4">Number of Transactions</p>
                      <p class="fs-30 mb-2">@ViewBag.numberOfTransactions</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!--Number Of End-->


          <!--Number Of Wallet in Each Bank Start-->
          <div class="row">
            <div class="col-md-12 mb-4">
              <div class="card">
                <div class="card-body">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Bank Name</th>
                                    <th>Number of Wallets</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var bank in ViewBag.WalletCountsByBank)
                                {
                                    <tr>
                                        <td>@bank.Key</td>
                                        <td>@bank.Value</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                </div>
              </div>
            </div>
          </div>
          <!--Number Of Wallet in Each Bank End-->

